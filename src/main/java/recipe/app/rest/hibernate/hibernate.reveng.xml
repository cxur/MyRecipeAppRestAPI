<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-schema="my_recipes" match-name="app_user" />
	<table-filter match-schema="my_recipes" match-name="category" />
	<table-filter match-schema="my_recipes" match-name="recipe" />
	<table-filter match-schema="my_recipes" match-name="recipe_category" />
</hibernate-reverse-engineering>